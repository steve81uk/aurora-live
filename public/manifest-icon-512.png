<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512">
  <defs>
    <radialGradient id="earthGrad" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#4ade80;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="auroraGrad" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:0" />
      <stop offset="70%" style="stop-color:#10b981;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" fill="#000000"/>
  
  <!-- Stars -->
  <circle cx="100" cy="100" r="2" fill="#ffffff" opacity="0.8"/>
  <circle cx="420" cy="80" r="1.5" fill="#ffffff" opacity="0.6"/>
  <circle cx="350" cy="450" r="1" fill="#ffffff" opacity="0.7"/>
  <circle cx="50" cy="300" r="2" fill="#ffffff" opacity="0.9"/>
  <circle cx="460" cy="350" r="1.5" fill="#ffffff" opacity="0.5"/>
  
  <!-- Aurora glow ring -->
  <circle cx="256" cy="256" r="200" fill="url(#auroraGrad)"/>
  
  <!-- Earth -->
  <circle cx="256" cy="256" r="150" fill="url(#earthGrad)"/>
  
  <!-- Cloud layer -->
  <ellipse cx="280" cy="220" rx="40" ry="30" fill="#ffffff" opacity="0.3"/>
  <ellipse cx="200" cy="280" rx="50" ry="35" fill="#ffffff" opacity="0.25"/>
  
  <!-- Aurora ring -->
  <circle cx="256" cy="256" r="170" fill="none" stroke="#10b981" stroke-width="3" opacity="0.6"/>
  <circle cx="256" cy="256" r="175" fill="none" stroke="#34d399" stroke-width="2" opacity="0.4"/>
  
  <!-- Text overlay -->
  <text x="256" y="480" font-family="Rajdhani, sans-serif" font-size="32" font-weight="700" fill="#ffffff" text-anchor="middle" letter-spacing="8">AETHERIS</text>
</svg>
